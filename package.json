{
  "name": "recat-material-dashboards",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start:webpack": "webpack serve --mode development",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint ./src",
    "test": "jest --watch --verbose=false",
    "build": "webpack --mode production",
    "start": "cross-env NODE_ENV=development parcel src/index.html --open",
    "build:parcel": "parcel build src/index.html --no-autoinstall --no-cache --no-source-maps && cp dist/index.html dist/404.html"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "@date-io/date-fns": "1.x",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.3",
    "@mui/lab": "^5.0.0-alpha.46",
    "@mui/material": "^5.0.3",
    "@mui/styles": "^5.0.1",
    "@reduxjs/toolkit": "^1.4.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "cross-env": "^7.0.3",
    "date-fns": "^2.12.0",
    "file-loader": "^6.2.0",
    "google-maps-react": "^2.0.2",
    "highcharts": "^8.0.0",
    "highcharts-react-official": "^2.2.2",
    "html-webpack-plugin": "^5.3.2",
    "moment": "^2.24.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.1.2",
    "recharts": "^2.0.0-beta.1",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "css-loader": "^6.4.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.25.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^1.6.0",
    "expect.js": "^0.3.1",
    "faker": "^4.1.0",
    "jest": "^23.6.0",
    "jest-extended": "^0.11.1",
    "parcel": "^2.0.0",
    "prettier": "^1.14.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
